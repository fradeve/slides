digraph G {
	graph [ dpi = 300 ];
	// table size
	size ="4,6";
	graph [fontsize=64];
	edge  [fontsize=64];
	node  [fontsize=64];
	ranksep = 5.5;
	nodesep = .25;
	edge [style="setlinewidth(3)"];
	layout=circo

	//// LABELING ////

	// non-os software
	autocad [label="AutoCAD"]
	arcgis [label="ArcGIS"]
	windows [label="MS Windows"]
	msserver [label="MS Server"]
	maya [label="Maya",shape=egg]
	msoffice [label="MS Office"]

	// destinations
	archeologo [label="archeologo"]

	//// CONNECTIONS ////

	archeologo -> autocad;
	archeologo -> arcgis;
	archeologo -> windows;
	archeologo -> msserver;
	archeologo -> maya;
	archeologo -> msoffice;
}
