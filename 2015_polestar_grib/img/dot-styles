\tikzstyle{sourcelabel} = [
    rectangle,
    draw=MidnightBlue,
    ultra thick,
    fill=black!0,
    text centered, rounded corners,
    minimum height=1.5em
]

\tikzstyle{datalabel} = [
    draw=black,
    text centered
    minimum height=1.5em
]

\tikzstyle{derivedlabel} = [
    rectangle,
    draw=YellowGreen!50,
    ultra thick,
    fill=black!0,
    text centered, rounded corners,
    minimum height=1.5em
]

\tikzstyle{comment} = [
    color=black!50,
    text centered,
    minimum height=1.5em
]
